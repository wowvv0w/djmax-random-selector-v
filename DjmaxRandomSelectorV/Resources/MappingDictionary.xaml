<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:v="clr-namespace:DjmaxRandomSelectorV.Views"
                    xmlns:vm="clr-namespace:DjmaxRandomSelectorV.ViewModels">
    <DataTemplate DataType="{x:Type vm:MainViewModel}">
        <v:MainView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FilterOptionIndicatorViewModel}">
        <v:FilterOptionIndicatorView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:FilterOptionViewModel}">
        <v:FilterOptionView/>
    </DataTemplate>
</ResourceDictionary>