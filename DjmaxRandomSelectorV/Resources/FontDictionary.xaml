<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <FontFamily x:Key="FontDefault">pack://application:,,,/Fonts/#Lato</FontFamily>
    <FontFamily x:Key="FontDefaultLight">pack://application:,,,/Fonts/#Lato Light</FontFamily>
    <FontFamily x:Key="FontDefaultBold">pack://application:,,,/Fonts/#Lato Black</FontFamily>
    <FontFamily x:Key="FontSpecial">pack://application:,,,/Fonts/#Bebas Neue</FontFamily>

    <FontFamily x:Key="LatoLightFont">pack://application:,,,/Fonts/#Lato Light</FontFamily>
    <FontFamily x:Key="LatoFont">pack://application:,,,/Fonts/#Lato</FontFamily>
    <FontFamily x:Key="LatoBlackFont">pack://application:,,,/Fonts/#Lato Black</FontFamily>
    <FontFamily x:Key="BebasNeueFont">pack://application:,,,/Fonts/#Bebas Neue</FontFamily>
    
</ResourceDictionary>